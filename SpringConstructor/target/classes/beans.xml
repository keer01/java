<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd">

	<bean id="emp" class="com.test.Student">

		<property name="id" value="101"></property>
		<property name="name" value="Rohit"></property>
		<property name="course" value="c"></property>
		<property name="fees" value="23444"></property>
	</bean>

	<bean id="emp1" class="com.test.Student">

		<constructor-arg name="id" value="201"></constructor-arg>
		<constructor-arg name="name" value="kohli"></constructor-arg>
		<constructor-arg name="course" value="java"></constructor-arg>
		<constructor-arg name="fees" value="234556"></constructor-arg>


	</bean>




</beans>

